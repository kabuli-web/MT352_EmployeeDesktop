<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.ComplaintDetailsController"
      prefHeight="400.0" prefWidth="600.0">
        <children>
            <Label text="LoginScreen" />

            <Label text="Complaint Title" />
            <TextField fx:id="title"/>
            <Label text="Complaint Message" />
            <TextField fx:id="message"/>
            <Label text="Complaint Resolved Status" />
            <CheckBox fx:id="resolveStatus"/>
            <Button text="Update" onAction="#UpdateComplaint"/>
            <VBox fx:id="ErrorList"/>
        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>

</VBox>
